{
  "name": "cs2-cfg-highlighter",
  "displayName": "CS2 CFG Highlighter",
  "description": "高亮 CS2 CFG檔中的關鍵字",
  "icon": "images/CS2-Logo.png",
  "publisher": "Yunkong",
  "version": "1.4.6",
  "engines": {
    "vscode": ">=1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Yunkong-ouo/Cs2CfgHighlighterExtension.git"
  },
  "enableTelemetry": false,
  "keywords": [
    "cs2",
    "csgo",
    "cfg",
    "highlight",
    "CS2高亮",
    "cs2highlight",
    "csgohighlight",
    "cs2 highlight",
    "csgo highlight",
    "cs"
  ],
  "contributes": {
    "languages": [
      {
        "id": "cs2 cfg",
        "aliases": [
          "CS2 CFG"
        ],
        "extensions": [
          ".cfg"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "csgo cfg",
        "aliases": [
          "CSGO CFG"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cs2 cfg",
        "scopeName": "source.cs2cfg",
        "path": "./syntaxes/cs2cfg.tmLanguage.json"
      },
      {
        "language": "csgo cfg",
        "scopeName": "source.csgocfg",
        "path": "./syntaxes/csgocfg.tmLanguage.json"
      }
    ]
  }
}
